{
  "name": "hue-2-frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:production": "EXPO_PUBLIC_ENVIRONMENT=production expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:production": "EXPO_PUBLIC_ENVIRONMENT=production expo start --android",
    "ios": "expo run:ios",
    "ios:production": "EXPO_PUBLIC_ENVIRONMENT=production expo start --ios",
    "web": "expo start --web",
    "web:production": "EXPO_PUBLIC_ENVIRONMENT=production expo start --web",
    "build:web": "expo export --platform web",
    "build:web:staging": "EXPO_PUBLIC_ENVIRONMENT=staging expo export --platform web",
    "build:web:production": "EXPO_PUBLIC_ENVIRONMENT=production expo export --platform web",
    "build:production": "EXPO_PUBLIC_ENVIRONMENT=production expo export --platform web",
    "postbuild:production": "cp assets/images/favicon.ico dist/favicon.ico && cp assets/images/favicon-48x48.png dist/favicon-48x48.png && cp assets/images/favicon-32x32.png dist/favicon-32x32.png && cp assets/images/favicon-16x16.png dist/favicon-16x16.png && cp assets/images/apple-touch-icon.png dist/apple-touch-icon.png",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:staging": "eas build --platform ios --profile staging",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:android:production": "eas build --platform android --profile production",
    "build:all:dev": "eas build --platform all --profile development",
    "build:all:staging": "eas build --platform all --profile staging",
    "build:all:production": "eas build --platform all --profile production",
    "build": "node scripts/build.js",
    "build:help": "node scripts/build.js --help",
    "lint": "expo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/slider": "^4.5.7",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@types/react-datepicker": "^6.2.0",
    "expo": "~53.0.0",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-modules-core": "~2.5.0",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "jwt-decode": "^3.1.2",
    "react": "19.0.0",
    "react-datepicker": "^8.5.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-slider": "^0.11.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.3",
    "@types/react": "~19.0.10",
    "babel-plugin-react-native-web": "^0.21.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.4.2",
    "react-test-renderer": "^19.1.1",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/__tests__/**",
      "!**/jest.setup.js",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/expo-env.d.ts",
      "!**/app/**",
      "!**/components/**",
      "!**/lib/features/**/*.tsx",
      "!**/lib/app-config.ts",
      "!**/lib/features/**/authService.ts",
      "!**/lib/features/**/notificationService.ts",
      "!**/lib/features/**/NotificationProvider.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "verbose": false,
    "silent": false
  },
  "description": "A modern habit tracking app with beautiful design and powerful analytics",
  "author": "adrien@ecoffet.com"
}
